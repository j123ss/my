<%= form_tag sessions_path do %>
  <div class="field email">
    <strong><%= label_tag :username, "User name:" %></strong>
    <%= text_field_tag :username, params[:username] %>
  </div>
  <div class="field">
    <strong><%= label_tag :password, "Password:" %></strong>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Log in" %>
<% end %>